<%= form_with url: "/teams/#{@team.id}/players", method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <br>
  <%= form.label :age %>
  <%= form.text_field :age %>
  <br>
  <%= form.label :injured %>
  <%= form.check_box :injured %>
  <br>
  <%= form.submit "Create Player" %>
<% end %>
