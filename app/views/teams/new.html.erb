<%= form_with url: "/teams", method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <br>
  <%= form.label :rank %>
  <%= form.text_field :rank %>
  <br>
  <%= form.label :won_championship %>
  <%= form.check_box :won_championship %>
  <br>
  <%= form.submit "Create Team" %>
<% end %>
